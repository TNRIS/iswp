		"eslint": "^8.28.0",
		"eslint-plugin-svelte": "^2.26.0",
		"node-sass": "^9.0.0",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"sass": "^1.64.2",
		"smui-theme": "^7.0.0-beta.16",
